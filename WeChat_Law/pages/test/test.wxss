page{  
  display: flex;  
  flex-direction: column;  
  height: 100%;  
}  
.weui-flex {
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.weui-cells {
  margin-top: 0;
  opacity: 0;
  -webkit-transform: translateY(-50%);
  transform: translateY(-50%);
  -webkit-transition: 0.3s;
  transition: 0.3s;
}

.weui-cells:after, .weui-cells:before {
  display: none;
}

.weui-cells_show {
  opacity: 1;
  -webkit-transform: translateY(0);
  transform: translateY(0);
}

.weui-cell:before {
  right: 15px;
}

.kind-list__item {
  background-color: #fff;
  border-radius: 2px;
  overflow: hidden;
}

.kind-list__item:first-child {
  margin-top: 0;
}

.kind-list__img {
  width: 30px;
  height: 30px;
}

.kind-list__item-hd {
  padding: 20px;
  -webkit-transition: opacity 0.3s;
  transition: opacity 0.3s;
}

.kind-list__item-hd_show {
  opacity: 0.4;
}

.kind-list__item-bd {
  height: 0;
  overflow: hidden;
}

.kind-list__item-bd_show {
  height: auto;
}





.xq_row
{
  display: flex;
  flex-direction: row;  
   font-size: 35rpx;  
}

.search{  
  flex: auto;  
  display: flex;  
  flex-direction: column;  
  background: #fff; 
  float:left; 
  width: 100%;
}  

/*搜索结果*/  
.search-result{  
  /* flex: auto;  
  position: relative;   */
  
}  
.search-result scroll-view{  
 
  bottom: 0;  
  left: 0;  
  right: 0;  
  top: 0;  
}  

.result-item{  
  position: relative;  
  display: flex;  
  flex-direction: column;  
  padding: 40rpx 60rpx 30rpx 60rpx;  
  overflow: hidden;  
  border-bottom: 2rpx solid #e5e5e5;  
}  

.result-item .title_txt, 
.result-item .subtitle{  
  overflow: hidden;  
  text-overflow: ellipsis;  
  white-space: nowrap;  
  line-height: 36rpx;  
}  

.result-sub{
  display: flex;  
  flex-direction: row;
  justify-content:space-between; 
}
.result-item .title{
  margin-bottom: 10rpx;
  display: flex;  
  line-height: 36rpx;  
}
.result-item .title_txt{  
 
  font-size: 35rpx; 
  color: #000;  
}  
.result-item .subtitle{  
  color: #808080;  
  font-size: 24rpx;  
}  
.result-item:first-child .subtitle text{  
  margin-right: 20rpx;  
}  

.result-item:not(:first-child) .subtitle text:not(:first-child):before{  
  content: '/';  
  margin: 0 8rpx;  
}  
.loading{  
  padding: 10rpx;  
  text-align: center;  
}  
.loading:before{  
  display: inline-block;  
  margin-right: 5rpx;  
  vertical-align: middle;  
  content: '';  
  width: 40rpx;  
  height: 40rpx;  
  background-size: contain;  
  animation: rotate 1s linear infinite;  
}  
.loading.complete:before{  
  display: none;  
}  


.search-wrap{  
  position: relative; 
  flex: auto;  
  display: flex; 
  align-items: center;  
  height: 80rpx;  
  padding: 0 20rpx;  
  background: #fff;  
  border-radius: 6rpx;  
}  
.search-wrap .icon-search{  
  margin-right: 10rpx;  
}  

.bgcolour{
    height: 100rpx;
   width: 100%;
  background:#e0e0e0;
}

.pickerwidth{
  width: 450rpx;
  overflow: hidden;  
  text-overflow: ellipsis;  
  white-space: nowrap;  
}
.icon{
  margin-left: 10rpx;
  width: 15px; 
  height: 15px;
}
.iconhidden{
  display: none;
}
.listhidden{
  display: none;
}
.last{
  width: 100%;
  display: flex;  
  flex-direction: row;
  background:#f3f3f3; 
  float:left;
  padding-bottom: 20rpx;
}
.last_bgic{
  width: 10rpx;
  height: 32rpx;
  background:#04be02; 
  margin: 8rpx 10rpx 0 55rpx;
}
.last_left{
  width:60%;
  display: flex;  
  flex-direction: row;
}
.last_right{
 display: flex;  
 align-items: center ;
}
.last_right text{
  width: 200rpx;
  text-overflow: ellipsis;  
  white-space: nowrap; 
  overflow: hidden;  
}
.last text{
 font-size: 32rpx;
 line-height: 48rpx;
 color: #666666;
 float:left;
 height: 48rpx;
}
.lastpu{
  height: 52rpx;
  padding-top: 20rpx;
}
.gettop{
  padding-left: 40%;
  padding-bottom: 10rpx;
  color: #666666;
  font-size: 35rpx;
}